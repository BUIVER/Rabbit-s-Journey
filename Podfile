    pod 'Alamofire' , '~> 4.7.2'
    pod 'AlamofireImage' , '~> 3.3.1'
    pod 'GoogleMaps'
    pod 'GooglePlaces'
    pod 'GooglePlacePicker'
    pod 'Firebase/Core'
    pod 'Firebase/Auth'
    pod 'Firebase/Firestore'
    pod 'Firebase/Storage'
end
